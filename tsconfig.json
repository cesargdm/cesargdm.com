{
	"compilerOptions": {
		"target": "esnext",
		"lib": ["dom", "esnext"],
		"jsx": "react",
		"module": "esnext",
		"rootDir": "./src",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true
	},
	"include": [
		"./src/**/*",
		"./gatsby-node.ts",
		"./gatsby-config.ts",
		"./src/.d.ts"
	]
}
